{
  "rewrites": [
    {
      "source": "/",
      "destination": "/browser"
    },
    {
      "source": "/:file",
      "has": [
        { "type": "header", "key": "Accept", "value": "text/css|application/json|application/javascript|text/plain" }
      ],
      "destination": "/browser/:file"
    },
    {
      "source": "/:file",
      "has": [
        { "type": "header", "key": "Accept", "value": "text/html" }
      ],
      "destination": "/browser"
    }
  ]
}
